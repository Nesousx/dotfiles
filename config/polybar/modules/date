[module/date]
type = internal/date
interval = 1

time = " %A %d %B - %H:%M"
#time-alt = " %a, %b %d %H:%M"

format-padding = 1
format-prefix = %{T3}Ôôè%{T-}
format-prefix-foreground = ${colors.nord-blue-light}
label = %{T2}%time%%{T-}
label-foreground = ${colors.nord-blue-light}

